<div data-controller="position" data-drag-url="/movies/:id/move" class="row">

<% @movies.order(position: :asc).each do |movie| %>
    <div id="<%= movie.id %>" class="col-sm-6 col-md-3" style="margin-top: 10px;">
     <%= link_to image_tag(movie.image, style:"width:150px; height:150px"), movie if movie.image.attached?%>
     </div> 
     <% end %>
     </div>